@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer


<div class="row">
    <div class="col-lg-3  col-md-3 col-sm-12 col-xs-12 form-group">
        <label for="cboAPStatutoryISDCode">@Localizer["ISDCode"]</label>
        <select class="selectpicker" data-container="body" data-live-search="true" id="cboAPStatutoryISDCode" title="" data-size="8" onchange="GetAPVendorStatutoryDetails()">
            <option value="0">@Localizer["Select"]</option>
        </select>
    </div>

    <div class="col-lg-3  col-md-3 col-sm-12 col-xs-12 form-group">
        <label for="cboAPVendorAddress">@Localizer["VendorLocationAddress"]</label>
        <select class="selectpicker" data-container="body" data-live-search="true" id="cboAPVendorAddress" title="" data-size="8" onchange="GetAPVendorStatutoryDetails()">
            <option value="0">@Localizer["Select"]</option>
        </select>
    </div>



</div>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 form-group">
        <table id="jqgAPVendorProfileStatutoryDetails"></table>
        <div id="jqpAPVendorProfileStatutoryDetails"></div>
    </div>
</div>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
        <hr />
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button" id="btnAPsavestatutory" onclick="fnSaveAPVendorStatutoryDetails()" style="margin-right:10px;">@Localizer["Save"]  </button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button" onclick="fnClearAPStatutoryDetails()">@Localizer["Clear"]  </button>
    </div>
</div>