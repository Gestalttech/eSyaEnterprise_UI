 @using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer

<div class="row padbot10 padtop15">
    <div class="col-lg-7 col-md-7 col-sm-12">

        <table id="jqgAPSupplyGroup"></table>
        <div id="jqpAPSupplyGroup"></div>

    </div>
</div>

<div class="row pb-3">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center form-group">
        <hr />
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button" id="btnApproveSupplyGroup" onclick="fnSaveAPSupplyGroup()" style="margin-right:10px;"><i class="fa fa-save"></i> @Localizer["Save"]  </button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button" onclick="fnRefreshAPSupplyGroupGrid()"><i class="fa fa-times"></i> @Localizer["Clear"] </button>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12  py-3 text-center bg-lightyellow border-dashed">
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button" id="btnApproveVendor" onclick="fnApproveVendor()"><i class="fa fa-check"></i> @Localizer["Approve"]  </button>
  </div>
</div>
