                    }
                    else
                    {
                        _logger.LogError(new Exception(serviceResponse.Message), "UD:TaxIdentification");
                    }
                }
                else
                {
                    _logger.LogError(new Exception(serviceResponse.Message), "UD:TaxIdentification");
                }
                return View();
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "UD:TaxIdentification");
                throw;
            }
        }
